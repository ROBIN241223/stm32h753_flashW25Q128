{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell: Windows PowerShell",
      "type": "PowerShell",
      "request": "launch",
      "sessionName": "Windows PowerShell (x64)"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/ducnt/workspace/h753duc/Core/Src",
      "program": "/home/ducnt/workspace/h753duc/Core/Src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
  ,
  {
    "name": "Cortex Debug (ST-Link)",
    "type": "cortex-debug",
    "request": "launch",
    "servertype": "stlink",
    "cwd": "${workspaceFolder}",
    "executable": "${workspaceFolder}/build/h753duc.elf",
    "device": "STM32H753IIKx",
    "runToEntryPoint": "main",
    "preLaunchTask": "Build Firmware"
  },
  {
    "name": "Flash STM32 (ST-Link)",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/build/h753duc.elf",
    "args": [],
    "stopAtEntry": false,
    "cwd": "${workspaceFolder}",
    "preLaunchTask": "Build Firmware",
    "miDebuggerServerAddress": "localhost:3333",
    "miDebuggerPath": "/usr/bin/arm-none-eabi-gdb",
    "setupCommands": [
      { "description": "Reset target", "text": "monitor reset halt" },
      { "description": "Load binary", "text": "load" },
      { "description": "Run", "text": "continue" }
    ],
    "externalConsole": false
  }
}